{
  "name": "casval-gas-station",
  "version": "1.3.0",
  "private": true,
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "test": "dotenv -e .env.test -- jest ./src  --runInBand --detectOpenHandles",
    "test:watch": "dotenv -e .env.test -- jest ./src --runInBand --watchAll --detectOpenHandles",
    "start": "node dist/server.js",
    "start:staging": "dotenv -e .env.staging -- node dist/server.js",
    "build": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"node dist/server.js\"",
    "release": "standard-version",
    "db:init": "prisma migrate dev --name init",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --preview-feature",
    "db:seed": "prisma db seed --preview-feature",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:test:migrate": "dotenv -e .env.test -- prisma migrate dev",
    "db:test:push": "dotenv -e .env.test -- prisma db push --preview-feature",
    "db:test:studio": "dotenv -e .env.test -- prisma studio",
    "db:test:seed": "dotenv -e .env.test -- prisma db seed --preview-feature",
    "db:staging:migrate": "dotenv -e .env.staging -- prisma migrate deploy",
    "db:staging:seed": "dotenv -e .env.staging -- prisma db seed --preview-feature",
    "db:production:migrate": "dotenv -e .env.production -- prisma migrate deploy",
    "db:production:status": "dotenv -e .env.production -- prisma migrate status",
    "db:production:seed": "dotenv -e .env.production -- prisma db seed --preview-feature"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ethersproject/experimental": "^5.1.1",
    "@prisma/client": "2.21.2",
    "axios": "^0.21.1",
    "env-schema": "^3.0.1",
    "ethers": "^5.0.32",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.9.1",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fluent-json-schema": "^2.0.4",
    "jsonwebtoken": "^8.5.1",
    "pusher-js": "^7.0.3",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.14",
    "@types/tap": "^14.10.1",
    "concurrently": "^6.0.0",
    "fastify-tsconfig": "^1.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "2.21.2",
    "pusher-js-mock": "^0.3.4",
    "standard-version": "^9.2.0",
    "tap": "^14.11.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "wait-for-expect": "^3.0.2"
  }
}
